def my_genrule(
    name,
    out,
    visibility = [],
  ):
  genrule(
    name = name,
    srcs = [
      'foo.txt',
      'bar.txt',
      '//fake:rule',
    ],
    bash = 'cat $SRCS > $OUT',
    out = out,
    visibility = visibility,
  )

my_genrule(
    name = "example",
    out = "baz.txt",
)

genrule(
    name = "foo",
    out = "foo.txt",
    cmd = "echo foo > $OUT",
    environment_expansion_separator = ",",
)

keystore(
    name = "debug_keystore",
    properties = "debug.keystore.properties",
    store = "debug.keystore",
)
