genrule(
    name = "cat_on_linux",
    srcs = [
        "b.txt",
    ],
    out = "cat_out.txt",
    cmd = "cat $SRCS > $OUT",
    cmd_exe = "type $SRCS > $OUT",
    target_compatible_with = [
        "//config:linux",
    ],
)

genrule(
    name = "cat_on_osx",
    srcs = [
        "a.txt",
    ],
    out = "cat_out.txt",
    cmd = "cat $SRCS > $OUT",
    cmd_exe = "type $SRCS > $OUT",
    target_compatible_with = [
        "//config:osx",
    ],
)
