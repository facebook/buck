for i in (2,3):
    genrule(
        name = "disassemble_big_r_dot_java_classes%d" % i,
        out = "big_r_dot_java_secondary.txt",
        bash = "$(exe //third-party/smali:baksmali) list classes " +
               "$(location :app_with_big_r_dot_java)/classes%d.dex > $OUT" % i,
    )
