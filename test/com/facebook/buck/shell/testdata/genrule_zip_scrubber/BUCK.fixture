script = 'create_zip.py'

genrule(
  name = 'genrule-one',
  out = 'output-one.zip',
  cmd = 'python $SRCS $OUT 0',
  srcs = [
    script
  ],
)

genrule(
  name = 'genrule-two',
  out = 'output-two.zip',
  cmd = 'python $SRCS $OUT 1',
  srcs = [
    script
  ],
)

genrule(
  name = 'extended-time-one',
  out = 'output.zip',
  cmd = 'cp $SRCS $OUT',
  srcs = [
    'extended_time.zip',
  ],
)

genrule(
  name = 'extended-time-two',
  out = 'output.zip',
  cmd = 'cp $SRCS $OUT',
  srcs = [
    'extended_time_2.zip',
  ],
)

