genrule(
    name = "main-fixed",
    out = "main-fixed.jar",
    cmd = " ".join([
        "$(exe //py/buck/zip:munger)",
        "--input=$(location :main)",
        "--output=$OUT",
        "--exclude=.api_description",
        "--exclude=.options",
        "--exclude=NOTICE",
        "--exclude=about.html",
        "--exclude=about_files",
        "--exclude=plugin.properties",
        "--exclude=plugin.xml",
        "--exclude=META-INF",
        "--include=META-INF/services",
        "--include=META-INF/native",
    ]),
    visibility = [
        "PUBLIC",
    ],
)
